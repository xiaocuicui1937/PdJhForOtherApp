                    android:layout_height="match_parent"
                    android:layout_marginLeft="2dp"
                    android:layout_weight="1.2"
                    android:background="@drawable/editsharp"
                    android:ems="10"
                    android:hint="@string/Feeddistance_input_hint"
                    android:text="30"
                    android:textSize="12sp" >

                    <requestFocus />
                </EditText>

                <Button
                    android:id="@+id/doFeedDot"
                    style="@style/TextStyle02"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.8"
                    android:background="@drawable/cornerbtn_style2"
                    android:text="@string/Feed_dot_btn"
                    android:textSize="16sp" />

                <Button
                    android:id="@+id/doFeedBack"
                    style="@style/TextStyle02"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="2dp"
                    android:layout_weight="0.8"
                    android:background="@drawable/cornerbtn_style2"
                    android:text="@string/Feed_back_btn"
                    android:textSize="16sp" />

                <Button
                    android:id="@+id/doBlackMarkCut"
                    style="@style/TextStyle02"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/cornerbtn_style"
                    android:text="@string/BlackMark_cut_btn"
                    android:textSize="16sp" />

                <Button
                    android:id="@+id/doHalfCut"
                    style="@style/TextStyle02"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/cornerbtn_style"
                    android:text="@string/Half_cut_btn"
                    android:textSize="16sp" />

                <Button
                    android:id="@+id/doTotalCut"
                    style="@style/TextStyle02"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/cornerbtn_style"
                    android:text="@string/Total_cut_btn"
                    android:textSize="16sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_codepage"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:gravity="center"
                android:orientation="horizontal" >

                <Spinner
                    android:id="@+id/CountrySpinner"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />
                <Button
                    android:id="@+id/SetCountryBtn"
                    style="@style/TextStyle02"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="3dp"
                    android:layout_weight="1.5"
                    android:background="@drawable/cornerbtn_style"
                    android:text="@string/Set_Country"
                    android:textSize="16sp" />

                <Spinner
                    android:id="@+id/CodePageSpinner"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp"
                    android:layout_weight="1" />
				<Button
                    android:id="@+id/SetCodePageBtn"
                    style="@style/TextStyle02"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="3dp"
                    android:layout_weight="1.5"
                    android:background="@drawable/cornerbtn_style"
                    android:text="@string/Set_CodePage"
                    android:textSize="16sp" />
                
                <Button
                    android:id="@+id/PrintCharPageBtn"
                    style="@style/TextStyle02"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp"
                    android:layout_weight="2"
                    android:background="@drawable/cornerbtn_style2"
                    android:text="@string/PrintCharPaperBtn"
                    android:textSize="16sp" />
            </LinearLayout>

            <TextView
                android:id="@+id/DisplayAdVlaue_tv"
                style="@style/TextStyle02"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.3"
                android:gravity="center"
                android:text="黑标AD值"
                android:textSize="12sp" />
            <!--
             <TextView
            android:id="@+id/HTSeat_tv"
            style="@style/TextStyle02"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:text="00 00 00" />
            -->
        </LinearLayout>
    </LinearLayout>

</LinearLayout>