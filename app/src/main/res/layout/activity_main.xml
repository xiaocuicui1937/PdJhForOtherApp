                    android:gravity="center"
                    android:hint="@string/First_hint"
                    android:inputType="number"
                    android:text="8"
                    android:textSize="14sp" />

                <EditText
                    android:id="@+id/SetColumn2_et"
                    android:layout_width="0dp"
